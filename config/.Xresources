! ===================
!       urxvt
! ===================

URxvt.scrollBar:	       false
URxvt.letterSpace:         -1
URxvt.internalBorder:      10
URxvt.font: 		       xft:RobotoMono Nerd Fontsize=12:minspace=False
URxvt.boldFont:		       xft:RobotoMono Nerd Fontsize=12:minspace=False
URxvt.cursorBlink:         true
URxvt.iso14755:		       false
URxvt.utf8:			       true
URxvt.keysym.C-equal:      resize-font:bigger
URxvt.keysym.C-parenright: resize-font:smaller

! =================
!       Xft
! =================
!Xft.dpi:         96
!Xft.antialias:   true
!Xft.rgba:		 none
!Xft.hinting:     true
!Xft.hintstyle:	 hintfull
! Xft.lcdfilter: lcddefault

! ============
!   Couleurs
! ============
